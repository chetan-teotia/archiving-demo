<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ArchiveTokBox</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>tokbox</servlet-name>
    <servlet-class>com.restSample.SessionServelet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>tokbox</servlet-name>
    <url-pattern>/SessionServelet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>tokboxarchive</servlet-name>
    <servlet-class>com.restSample.ArchiveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>tokboxarchive</servlet-name>
    <url-pattern>/archive</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>tokboxarchivestatus</servlet-name>
    <servlet-class>com.restSample.ServletArchiveStatus</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>tokboxarchivestatus</servlet-name>
    <url-pattern>/archivecallback</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>callback</servlet-name>
    <servlet-class>com.restSample.callbackresponse</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>callback</servlet-name>
    <url-pattern>/archivestatus</url-pattern>
  </servlet-mapping>
</web-app>